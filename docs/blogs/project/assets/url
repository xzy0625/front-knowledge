<script type="text/javascript" src="./assets/public/js/qrcode.js?v=djMuNDEuMF8zMjhmYjE3MDg3OTI5MDIzZmFkY2Y2NTFjNDE4ZDNlMGQ0MGM0Mzlm"></script>

    <script type="text/javascript" src="./assets/public/js/banner.js?v=djMuNDEuMF8zMjhmYjE3MDg3OTI5MDIzZmFkY2Y2NTFjNDE4ZDNlMGQ0MGM0Mzlm"></script>


    <script type="text/javascript" src="./assets/public/js/app.js?v=djMuNDEuMF8zMjhmYjE3MDg3OTI5MDIzZmFkY2Y2NTFjNDE4ZDNlMGQ0MGM0Mzlm"></script>
    



<script type="text/javascript" src="./assets/public/js/darkreader.js?v=djMuNDEuMF8zMjhmYjE3MDg3OTI5MDIzZmFkY2Y2NTFjNDE4ZDNlMGQ0MGM0Mzlm"></script>
<script>
  $(document).ready(function() {
    function onDarkModeChange() {
      if (DarkReader.isEnabled()) {
        $(".TDesign-theme-tabs .moon").addClass("active");
        $(".TDesign-theme-tabs .sun").removeClass("active");
        $(".TDesign-theme-tabs").addClass("is-dark");
      } else {
        $(".TDesign-theme-tabs .sun").addClass("active");
        $(".TDesign-theme-tabs .moon").removeClass("active");
        $(".TDesign-theme-tabs").removeClass("is-dark");
      }
    }

    var darkFix = {
      invert: ['.dropbtn'],
      css: '\
        body {background-color: #000; }\
        .login_quick .selected {\
            color: #0052D9;\
        }\
        .quick_login .login_normal { background-color: rgb(29 30 31); } \
        .login_normal .card-box { background-color: rgb(46 49 51); border-color: rgb(77 81 86); } \
        .login_normal .connect-line { filter: brightness(3); } \
      ',
      ignoreInlineStyle: ['.darkmode-ignore', '.darkmode-ignore *'],
      ignoreImageAnalysis: [],
    };

    $(".TDesign-theme-tabs .sun").click(function() {
      DarkReader.disable();
      onDarkModeChange();
    });

    $(".TDesign-theme-tabs .moon").click(function() {
      DarkReader.enable({}, darkFix);
      onDarkModeChange();
    });

    function handleDarkMode() {
      var darkMode = window.matchMedia("(prefers-color-scheme: dark)");
      
      darkMode.addListener(function() {
        setTimeout(function() {
          onDarkModeChange();
        }, 0);
      })

      
      DarkReader.auto({}, darkFix);
      onDarkModeChange();
    }

    
    if (window.top === window.self) {
      handleDarkMode();
    } else {
      
      var darkMode = window.location.search.toLowerCase().match(/darkmode=(\w+)/);
      console.log('darkmode:', darkMode);
      if (darkMode) {
        switch (darkMode[1]) {
          case 'auto':
            handleDarkMode();
            break;
          case 'true':
            DarkReader.enable({}, darkFix);
            onDarkModeChange();
            break;
          case 'false':
            DarkReader.disable();
            onDarkModeChange();
            break;
          default:
            
        }
      }
    }
  });
</script>

</body>
</html>
